{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/admin/upload/route.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "GOOGLE_MODEL": "gemini-2.0-flash-exp",
    "EMBEDDING_DIM": "1024",
    "TOP_K": "5"
  }
}
