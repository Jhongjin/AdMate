{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GOOGLE_MODEL": "gemini-2.0-flash-exp",
    "EMBEDDING_DIM": "768",
    "TOP_K": "5"
  }
}
